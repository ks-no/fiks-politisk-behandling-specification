{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "no.ks.fiks.politisk.behandling.v1.utvalgssak.send.schema.json",
    "title": "SendUtvalgssak",
    "definitions": {
        "Behandlingsplan": {
            "type": "object",
            "properties": {
                "tilUtvalgId": {
                    "type": "string"
                },
                "tilUtvalgNavn": {
                    "type": "string"
                },
                "tilMoeteId": {
                    "type": "string"
                },
                "tilMoetedato": {
                    "type": "string",
                    "format": "date-time"
                },
                "rekkefoelge": {
                    "type": "integer"
                }
            },
            "required": [
                "tilUtvalgId",
                "tilUtvalgNavn"
            ]
        }
    },
    "type": "object",
    "properties": {
        "sak": {
            "type": "object",
            "properties": {
                "referanseEksternNoekkel": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.eksternnoekkel.schema.json"
                },
                "fagsystemetsSaksnummer": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.saksnummer.schema.json"
                },
                "fagsystemetsReferanseEksternNoekkel": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.eksternnoekkel.schema.json"
                },
                "tittel": {
                    "type": "string"
                },
                "saksbeskrivelse": {
                    "type": "object",
                    "properties": {
                        "tekstInnhold": {
                            "type": "string"
                        },
                        "tekstFormat": {
                            "type": "object",
                            "properties": {
                                "kodeverdi": {
                                    "type": "string"
                                },
                                "kodebeskrivelse": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "forslagTilVedtak": {
                    "type": "object",
                    "properties": {
                        "tekstInnhold": {
                            "type": "string"
                        },
                        "tekstFormat": {
                            "type": "object",
                            "properties": {
                                "kodeverdi": {
                                    "type": "string"
                                },
                                "kodebeskrivelse": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "skjerming": {
                    "type": "object",
                    "properties": {
                        "skjermingshjemmel": {
                            "type": "string"
                        },
                        "skjermingsvarighet": {
                            "type": "integer"
                        },
                        "skjermingOpphoererDato": {
                            "type": "string",
                            "format": "date"
                        }
                    },
                    "required": [
                        "skjermingshjemmel"
                    ]
                },
                "saksansvarlig": {
                    "type": "string"
                },
                "posisjon": {
                    "description": "Punkt",
                    "type": "object"
                },
                "referanseForelderMappeEksternNoekkel": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.eksternnoekkel.schema.json"
                },
                "referanseForelderMappe": {
                    "type": "string"
                },
                "arkivSaksnummer": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.saksnummer.schema.json"
                },
                "referanseRegistreringEksternNoekkel": {
                    "$ref": "no.ks.fiks.politisk.behandling.v1.felles.eksternnoekkel.schema.json"
                },
                "referanseRegistrering": {
                    "type": "string"
                },
                "arkivJournalnummer": {
                    "type": "object",
                    "properties": {
                        "journalaar": {
                            "type": "integer"
                        },
                        "journalsekvensnummer": {
                            "type": "integer"
                        }
                    }
                },
                "arkivJournalpostnummer": {
                    "type": "integer"
                }
            },
            "required": [
                "referanseEksternNoekkel",
                "fagsystemetsReferanse",
                "fagsystemetsSaksnummer",
                "tittel",
                "saksansvarlig"
            ]
        },
        "forslagTilBehandlingsplan": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/Behandlingsplan"
            },
            "minItems": 1
        }
    },
    "required": [
        "sak",
        "forslagTilBehandlingsplan"
    ]
}
